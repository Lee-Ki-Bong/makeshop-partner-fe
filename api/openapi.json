          },
          "marketingSms": {
            "type": "boolean",
            "description": "광고성 정보 수신 동의 SMS",
            "example": true
          },
          "marketingEmail": {
            "type": "boolean",
            "description": "광고성 정보 수신 동의 EMAIL",
            "example": true
          }
        },
        "required": [
          "partnerName",
          "representativeImage",
          "homepageUrl",
          "representativeName",
          "email",
          "phone",
          "postalCode",
          "baseAddress",
          "detailAddress",
          "introduction",
          "marketingSms",
          "marketingEmail"
        ]
      },
      "AdminUpdatePartnerDto": {
        "type": "object",
        "properties": {
          "partnerName": {
            "type": "string",
            "example": "홍길동",
