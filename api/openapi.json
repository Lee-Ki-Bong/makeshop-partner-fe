            "type": "string",
            "description": "전화번호",
            "example": "010-1234-5678"
          },
          "postalCode": {
            "type": "string",
            "description": "우편번호",
            "example": "04524"
          },
          "baseAddress": {
            "type": "string",
            "description": "기본 주소",
            "example": "서울시 중구 세종대로 110"
          },
          "detailAddress": {
            "type": "string",
            "description": "상세 주소",
